{
  "name": "skale",
  "version": "1.2.1",
  "license": "Apache-2.0",
  "description": "parallel and distributed data processing engine",
  "main": "index.js",
  "scripts": {
    "start": "pf=/tmp/skale-server.pid; test -f $pf && exit 1; bin/server.js -l 0 & echo $! > $pf",
    "stop": "pf=/tmp/skale-server.pid; test -f $pf || exit 1; kill $(cat $pf); rm -f $pf",
    "pretest": "eslint .",
    "tape": "tape \"test/**/*.js\"",
    "test": "make -C test"
  },
  "bin": {
    "skale-server": "./bin/server.js",
    "skale-worker": "./bin/worker.js",
    "skale-shell": "./bin/shell.js"
  },
  "repository": "skale-me/skale",
  "bugs": {
    "url": "https://github.com/skale-me/skale/issues"
  },
  "keywords": [
    "big data",
    "ETL",
    "distributed",
    "data processing",
    "machine learning",
    "cloud",
    "S3",
    "azure",
    "parallel",
    "cluster",
    "hpc"
  ],
  "author": "Skale team",
  "dependencies": {
    "await-outside": "^2.1.2",
    "aws-sdk": "^2.229.1",
    "azure-storage": "^2.8.2",
    "browserify": "^16.2.0",
    "callsite": "^1.0.0",
    "merge2": "^1.2.1",
    "micromatch": "^3.1.10",
    "mkdirp": "^0.5.1",
    "node-getopt": "^0.3.2",
    "resolve": "^1.7.1",
    "rimraf": "^2.6.2",
    "seedrandom": "^2.4.3",
    "thenify": "^3.3.0",
    "uuid": "^3.2.1",
    "websocket-stream": "^5.1.2",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "plotter": "^0.5.0",
    "tape": "^4.9.0"
  },
  "peerDependencies": {
    "node-parquet": "^0.2.4"
  },
  "engines": {
    "node": ">=6.0"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-var": 2,
      "no-console": 0
    },
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "extends": "eslint:recommended"
  }
}
